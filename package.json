  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-1"
    ],
    "env": {
      "dev": {
        "presets": [
          "react-hmre"
        ]
      }
    }
  },
  "eslintConfig": {
    "rules": {
      "indent": [
        2,
        2
      ],
      "quotes": [
        2,
        "single"
      ],
      "linebreak-style": [
        2,
        "unix"
      ],
      "semi": [
        2,
        "always"
      ],
      "react/jsx-uses-react": 2,
      "react/jsx-uses-vars": 2,
      "react/react-in-jsx-scope": 2
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    },
    "plugins": [
      "react",
      "transform-decorators-legacy"
    ]
  },
  "main": "hot.proxy.js",
  "devDependencies": {
    "babel-plugin-transform-runtime": "^6.23.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/01laky/isomorphic-go-react-sandbox.git"
  },
  "keywords": [
    "isomorfic-go-react-sandbox"
